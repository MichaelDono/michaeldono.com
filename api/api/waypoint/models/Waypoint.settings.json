{
  "connection": "default",
  "collectionName": "waypoint",
  "info": {
    "name": "waypoint",
    "description": "A description for a waypoint on the Newcastle Tour"
  },
  "options": {
    "timestamps": [
      "createdAt",
      "updatedAt"
    ]
  },
  "attributes": {
    "name": {
      "required": true,
      "type": "string"
    },
    "content": {
      "required": true,
      "type": "text"
    },
    "image": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": true
    },
    "number": {
      "type": "integer",
      "unique": true,
      "required": true
    }
  }
}